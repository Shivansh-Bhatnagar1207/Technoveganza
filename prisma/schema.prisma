generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  EventID Int     @id @unique
  Event1  Boolean
  Event2  Boolean
  Event3  Boolean
  Event4  Boolean
  Event5  Boolean
  Event6  Boolean
  Event7  Boolean
  PID     PID     @relation(fields: [EventID], references: [id])
  TID     TID?
}

model PID {
  id        Int      @id @default(autoincrement())
  Pid       Int      @unique
  Name      String
  RollNo    String   @unique
  Mobile    String   @unique
  Branch    String
  Course    String
  College   String
  Gender    String
  createdAt DateTime @default(now())
  Event     Event?
  TID       TID[]
}

model TID {
  id    Int   @id @default(autoincrement())
  Event Event @relation(fields: [id], references: [EventID])
  PID   PID[]
}
